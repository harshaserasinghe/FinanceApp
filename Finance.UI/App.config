<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework"
      type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"
      requirePermission="false"/>
  </configSections>
  <connectionStrings>
    <add name="financeDbContext"
          providerName="System.Data.SqlClient"
          connectionString="Server=localhost;Database=FinanceDb;Integrated Security=True;"/>
  </connectionStrings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
  </startup>
  <entityFramework>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer"/>
    </providers>
  </entityFramework>
  <appSettings>
    <add key="TranPath" value="d:\\FinanaceApp\\Transactions" />
    <add key="ContPath" value="d:\\FinanaceApp\\Contacts" />
    <add key="EvntPath" value="d:\\FinanaceApp\\Events" />
    <add key="UserPath" value="d:\\FinanaceApp\\Users" />
  </appSettings>
</configuration>